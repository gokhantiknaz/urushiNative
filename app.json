{
  "expo": {
    "name": "Myth Lights",
    "slug": "MythLights",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "Light",
    "icon": "./assets/iconTrans.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gokhantiknaz.MythLights"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconTrans.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gokhantiknaz.MythLights",
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/iconTrans.png"
    },
    "extra": {
      "eas": {
        "projectId": "0d92669c-43fd-4af7-a4a4-4a6abfa0dd6a"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "@config-plugins/react-native-ble-plx",
        {
          "isBackgroundEnabled": true,
          "modes": [
            "peripheral",
            "central"
          ],
          "bluetoothAlwaysPermission": "Allow $(PRODUCT_NAME) to connect to bluetooth devices"
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0d92669c-43fd-4af7-a4a4-4a6abfa0dd6a"
    }
  }
}
